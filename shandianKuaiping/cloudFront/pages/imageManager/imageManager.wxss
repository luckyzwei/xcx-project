page{
  height: 100vh;
}
.root{
  height: 100vh;
  /* overflow: auto; */
  position: relative;
}


.navigation_bar {
  width: 100%;
  height: 64px;
  position: fixed;
  text-align: center;
  background: #FED93B;
  z-index: 99999;
}

.navigation_bar .back {
  position: absolute;
  left: 19rpx;
  bottom: 14rpx;
  width: 60rpx;
  height: 60rpx;
}


.navigation_bar .title {
  left: 40%;
  width: 144rpx;
  bottom: 9.5px;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  height: 50rpx;
  position: absolute;
}


.test {
  margin-left: 5%;
  width: 90%;
  padding-bottom: 140rpx;
}



.main_canvas {
  width: 100%;
}

.bottom {
  position: fixed;
  width: 100%;
  height: 140rpx;
  bottom: 0;
  left: 0;
  z-index: 10000;
  background: #FFFFFF;
box-shadow: 0 -4rpx 10rpx 0 rgba(0,0,0,0.05);
display: flex;
align-items: center;
}

.bottom .mosaic {
  width: 60rpx;
  height: 140rpx;
  margin-left: 37rpx;
  text-align: center;
}

.bottom .mosaic .icon {
  margin-top: 38rpx;
  width: 46rpx;
  height: 46rpx;
}

.bottom .mosaic .desc {
  margin-top: 7rpx;
  font-family: PingFangSC-Regular;
font-size: 20rpx;
color: #000000;
letter-spacing: 0;
text-align: center;
width: 60rpx;
height: 28rpx;
}

.bottom .submit {
  /* margin-left: 510rpx; */
  margin-left: 413rpx;
  background: #FED93B;
box-shadow: 0 7rpx 14rpx 0 rgba(0,0,0,0.14);
border-radius: 50rpx;
text-align: center;
font-family: PingFangSC-Medium;
font-size: 30rpx;
color: #000000;
width: 200rpx;
height: 80rpx;
line-height: 80rpx;
}

