page{
  height: 100vh;
}
.root{
  height: 100vh;
  overflow: hidden;
  position: relative;
}


.navigation_bar {
  width: 100%;
  height: 64px;
  position: relative;
  text-align: center;
  background: #FED93B;
  z-index: 99999;
}

.navigation_bar .back {
  position: absolute;
  left: 19rpx;
  bottom: 14rpx;
  width: 60rpx;
  height: 60rpx;
}


.navigation_bar .title {
  left: 40%;
  width: 144rpx;
  bottom: 9.5px;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  height: 50rpx;
  position: absolute;
}

scroll-view {
  width: 100;
  background: #f3f3f3;
}
