page{
  height: 100vh;
}
.root{
  height: 100vh;
  
  position: relative;
}


.navigation_bar {
  width: 100%;
  height: 64px;
  position: relative;
  text-align: center;
  background: #FED93B;
  z-index: 99999;
}

.navigation_bar .back {
  position: absolute;
  left: 19rpx;
  bottom: 14rpx;
  width: 60rpx;
  height: 60rpx;
}


.navigation_bar .title {
  left: 40%;
  width: 144rpx;
  bottom: 9.5px;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  height: 50rpx;
  position: absolute;
}

canvas {
  width: 100%;
  
}


.bottom {
  width: 100%;
  height: 180rpx;
  background: #fff;
  box-shadow: 0 -4rpx 10rpx 0 rgba(0,0,0,0.05);
  position: fixed;
  bottom: 0;
  align-items: center;
}

.bottom .share {
  background: #FED93B;
  box-shadow: 0 7rpx 14rpx 0 rgba(0,0,0,0.14);
  border-radius: 50rpx;
  margin: 40rpx auto;
  width: 340rpx;
  height: 100rpx;
  align-items: center;
  display: flex;
}

.bottom .share cover-image {
  margin-left: 33rpx;
  margin-right: 11rpx;
  width: 54rpx;
  height: 54rpx;
  
}

.bottom .share cover-view {
  height: 100rpx;
  line-height: 100rpx;
  font-family: PingFangSC-Medium;
  font-size: 34rpx;
  color: #000000;
  text-align: left;
  margin-left: 11rpx;
}
