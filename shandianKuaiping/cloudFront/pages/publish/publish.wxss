page{
  height: auto;
}
.root{
  height: auto;
  position: relative;
}

.shadow_view {
  position: absolute;
  z-index: 1000;
  width: 100%;
  height: 120%;
  background: rgba(0, 0, 0, 0.2);
}

.sheet {
  width: 560rpx;
  height: 270rpx;
  background: #FFFFFF;
  box-shadow: 0 7rpx 14rpx 0 rgba(0,0,0,0.14);
  border-radius: 10rpx;
  margin: 600rpx auto;
  display: flex;
}

.sheet cover-view {
  width: 280rpx;
  height: 100%;
  text-align: center;
}

.sheet cover-view .coverImage {
  margin: 50rpx auto 0;
  width: 110rpx;
  height: 110rpx;
}

.sheet cover-view .coverView {
  font-family: PingFangSC-Medium;
font-size: 30rpx;
color: #000000;
letter-spacing: 0;
margin: 20rpx auto 0;
}

.navigation_bar {
  width: 100%;
  height: 64px;
  position: relative;
  text-align: center;
  background: #FED93B;
  z-index: 99999;
}

.navigation_bar .back {
  position: absolute;
  left: 19rpx;
  bottom: 14rpx;
  width: 60rpx;
  height: 60rpx;
}


.navigation_bar .title {
  left: 40%;
  width: 144rpx;
  bottom: 9.5px;
  font-family: PingFangSC-Medium;
  font-size: 36rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  height: 50rpx;
  position: absolute;
}

.scroll-view {
  width: 100;
}

.scroll-view .title {
  width: 690rpx;
  height: 120rpx;
  margin: 50rpx 30rpx 0;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: left;
  overflow: hidden;
}

.scroll-view .line {
  margin: 20rpx 30rpx 0;
  height: 1rpx;
  width: 690rpx;
  background: #e6e6e6;
}

.info {
  width: 690rpx;
  height: 350rpx;
  margin: 40rpx 30rpx 0;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #000000;
  text-align: left;
  line-height: 49rpx;
}

.image_background {
  width: 690rpx;
  height: 690rpx;
  margin: 10rpx 30rpx 0;
  padding-bottom: 200rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.image_background>view {
  width: 224rpx;
  height: 224rpx;
  position: relative;
}

.image_background .image>.delete {
  position: absolute;
  z-index: 100;
  right: 0;
  top: 0;
  width: 36rpx;
  height: 36rpx;
}

.image_background .image>.shadow {
  position: absolute;
  z-index: 101;
  width: 224rpx;
  height: 224rpx;
  background: rgba(0, 0, 0, 0.5);
}

.image_background .image>.stick {
  position: absolute;
  z-index: 102;
  margin: 67rpx;
  width: 90rpx;
  height: 90rpx;
  border-radius: 90rpx;
  background: #fff;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #000;
  text-align: center;
  line-height: 90rpx;
}


.image_background .image>.photo {
  width: 224rpx;
  height: 224rpx;
}



.image_background .add_pic>image {
  width: 224rpx;
  height: 224rpx;
}


.bottom {
  position: fixed;
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: flex-end;
  background: #fff;
  bottom: 0;
  z-index: 101;
}

.bottom>.cancel {
  background: #E8E8E8;
border-radius: 50rpx;
width: 200rpx;
height: 100rpx;
margin-right: 30rpx;
font-family: PingFangSC-Regular;
font-size: 34rpx;
line-height: 100rpx;
color: #909090;
text-align: center;
margin-top: 40rpx;
}


.bottom>.submit {
  background: #FED93B;
  box-shadow: 0 7rpx 14rpx 0 rgba(0,0,0,0.14);
  border-radius: 50rpx;
  margin-right: 40rpx;
  width: 200rpx;
  height: 100rpx;
  text-indent: 105rpx;
  line-height: 100rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #000;
  position: relative;
  margin-top: 40rpx;
}

.bottom .submit image {
  position: absolute;
  z-index: 1;
  width: 54rpx;
  height: 48rpx;
  left: 38rpx;
  top: 26rpx;
}

.bottom .anony {
  left: 40rpx;
  top: 59rpx;
  width: 72rpx;
  height: 52rpx;
  position: absolute;
}


.gap {
  width: 100%;
  height: 120rpx;
}
