.thumbAnimation {
  animation: thumbAnimation 0.3s infinite;
}


@keyframes leftAnimation {
  0% {width: 0}
  100%{flex: auto}
}
@keyframes rightAnimation {
  0% {width: 0}
  100%{flex: auto}
}

@keyframes commentBgAnimation {
  0% {margin-top: 100vh}
  100% {margin-top: calc(56rpx + 100px)}
}

@keyframes refreshAnimation {
  0% {transform:rotateZ(0deg);}
  5% {transform:rotateZ(18deg);}
  10% {transform:rotateZ(36deg);}
  15% {transform:rotateZ(54deg);}
  20% {transform:rotateZ(72deg);}
  25% {transform:rotateZ(90deg);}
  30% {transform:rotateZ(108deg);}
  35% {transform:rotateZ(126deg);}
  40% {transform:rotateZ(144deg);}
  45% {transform:rotateZ(162deg);}
  50% {transform:rotateZ(180deg);}
  55% {transform:rotateZ(198deg);}
  60% {transform:rotateZ(216deg);}
  65% {transform:rotateZ(234deg);}
  70% {transform:rotateZ(252deg);}
  75% {transform:rotateZ(270deg);} 
  80% {transform:rotateZ(288deg);}
  85% {transform:rotateZ(306deg);}
  90% {transform:rotateZ(324deg);}
  95% {transform:rotateZ(342deg);}
  100% {transform:rotateZ(360deg);}
}

@keyframes countAnimation {
  0% {margin-top: 40rpx;}
  50% {margin-top: 45rpx;}
  100% {margin-top: 40rpx;}    
}

@keyframes thumbAnimation {
  0% {transform:scale(1.0,1.0)}
  5% {transform:scale(1.03,1.03)}
  10% {transform:scale(1.06,1.06)}  
  15% {transform:scale(1.09,1.09)}
  20% {transform:scale(1.12,1.12)}
  25% {transform:scale(1.15,1.15)}
  30% {transform:scale(1.18,1.18)}
  35% {transform:scale(1.21,1.21)} 
  40% {transform:scale(1.24,1.24)}
  45% {transform:scale(1.27,1.27)}
  50% {transform:scale(1.3,1.3)}  
  55% {transform:scale(1.27,1.27)}
  60% {transform:scale(1.24,1.24)}
  65% {transform:scale(1.21,1.21)}
  70% {transform:scale(1.18,1.18)} 
  75% {transform:scale(1.15,1.15)}
  80% {transform:scale(1.12,1.12)}
  85% {transform:scale(1.09,1.09)}
  90% {transform:scale(1.06,1.06)} 
  95% {transform:scale(1.03,1.03)} 
  100% {transform:scale(1.0,1.0)}
}


